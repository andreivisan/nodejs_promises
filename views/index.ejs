<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <a href="/get-cached-users">Get Cached Users</a>
    <ul>
      <% if(locals.users) {%>
        <% users.forEach( function( user ) { %>
          <li>
            <%= user %>
          </li>
        <% }) %>
      <% } %>
    </ul>
  </body>
</html>
